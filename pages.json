{
  "easycom": {
    "^u-(.*)": "@/uview-ui/components/u-$1/u-$1.vue"
  },
  "pages": [
    {
      "path": "pages/index/index",
      "style": {
        "navigationBarTitleText": ""
      }
    },
    {
      "path": "pages/official/home",
      "style": {
        "navigationBarTitleText": "渝中区"
      }
    },
    {
      "path": "pages/official/monitor",
      "style": {
        "navigationBarTitleText": "实时监控"
      }
    }
